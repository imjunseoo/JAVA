import java.util.*; 

public class main {

	public static void main(String[] args) 
	{
		Scanner input = new Scanner(System.in);
		
		int v1 = input.nextInt();	
		int v2 = ~v1 + 1;
		String bin = Integer.toBinaryString(v1);
		String bin3 = Integer.toBinaryString(v2);
		System.out.println(bin + "십진수" + v1);
		System.out.println(bin3 + "십진수" + v2);
			
	}
}

